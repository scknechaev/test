<!DOCTYPE html>
<html lang="en" data-ng-app="publicApp" ng-controller="AppCtrl">

<head>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <meta charset="utf-8" />
    <title ng-bind="app.name"></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <link rel="shortcut icon" href="/publicApp/favicon.ico">
    <link rel="apple-touch-icon" href="pages/ico/60.png">
    <link rel="apple-touch-icon" sizes="76x76" href="pages/ico/76.png">
    <link rel="apple-touch-icon" sizes="120x120" href="pages/ico/120.png">
    <link rel="apple-touch-icon" sizes="152x152" href="pages/ico/152.png">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-touch-fullscreen" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="default">
    <meta content="{{app.description}}" name="description" />
    <meta content="{{app.author}}" name="author" />
    <!-- BEGIN Vendor CSS-->
    <script src="bower_components/momentjs/min/moment-with-locales.min.js"></script>
    <link href="bower_components/ng-notify/src/styles/ng-notify.css" rel="stylesheet" type="text/css" />
    <link href="dependencies/plugins/pace/pace-theme-flash.css" rel="stylesheet" type="text/css" />
    <link href="dependencies/plugins/boostrapv3/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="dependencies/plugins/font-awesome/css/font-awesome.css" rel="stylesheet" type="text/css" />
    <link href="dependencies/plugins/jquery-scrollbar/jquery.scrollbar.css" rel="stylesheet" type="text/css" media="screen" />
    <link id="lazyload_placeholder">
    <!-- BEGIN Pages CSS-->
    <link href="/publicApp/pages/css/pages-icons.css" rel="stylesheet" type="text/css">
    <link href="/publicApp/pages/css/awesome.css" rel="stylesheet" type="text/css">
    <link class="main-stylesheet" ng-href="{{app.layout.theme}}" rel="stylesheet" type="text/css" />
    <!--[if lte IE 9]>
        <link href="/publicApp/pages/css/ie9.css" rel="stylesheet" type="text/css" />
    <![endif]-->
    <!--[if lt IE 9]>
            <link href="dependencies/plugins/mapplic/css/mapplic-ie.css" rel="stylesheet" type="text/css" />
    <![endif]-->
    <script type="text/javascript">
    window.onload = function() {
        // fix for windows 8
        if (navigator.appVersion.indexOf("Windows NT 6.2") != -1)
            document.head.innerHTML += '<link rel="stylesheet" type="text/css" href="pages/css/windows.chrome.fix.css" />'
    }
    </script>
</head>

<body class="fixed-header" ng-class="{
'bg-master-lighter': is('app.extra.timeline'),
'no-header': is('app.social') || is('app.calendar') || is('app.maps.vector') || is('app.maps.google'),
'menu-pin' : app.layout.menuPin,
'menu-behind' : app.layout.menuBehind
 }">
    <div class="full-height" ui-view></div>
    <!-- BEGIN VENDOR JS -->
    <!-- JQUERY -->
    <script src="dependencies/plugins/pace/pace.min.js" type="text/javascript"></script>
    <script src="dependencies/plugins/jquery/jquery-1.11.1.min.js" type="text/javascript"></script>
    <script src="dependencies/plugins/modernizr.custom.js" type="text/javascript"></script>
    <script src="dependencies/plugins/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>
    <script src="dependencies/plugins/boostrapv3/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="dependencies/plugins/jquery/jquery-easy.js" type="text/javascript"></script>
    <script src="dependencies/plugins/jquery-unveil/jquery.unveil.min.js" type="text/javascript"></script>
    <script src="dependencies/plugins/jquery-bez/jquery.bez.min.js"></script>
    <script src="dependencies/plugins/jquery-ios-list/jquery.ioslist.min.js" type="text/javascript"></script>
    <script src="dependencies/plugins/jquery-actual/jquery.actual.min.js"></script>
    <script src="dependencies/plugins/jquery-scrollbar/jquery.scrollbar.min.js"></script>
    <script type="text/javascript" src="dependencies/plugins/classie/classie.js"></script>
    <!-- ANGULAR -->
    <script src="dependencies/plugins/angular/angular.js" type="text/javascript"></script>
    <script src="dependencies/plugins/angular-ui-router/angular-ui-router.min.js" type="text/javascript"></script>
    <script src="dependencies/plugins/angular-ui-util/ui-utils.min.js" type="text/javascript"></script>
    <script src="dependencies/ct-ui-router-extras.js"></script>
    <script src="/bower_components/ng-notify/dist/ng-notify.min.js"></script>
    <script src="/bower_components/angular-bootstrap/ui-bootstrap-tpls.js"></script>
    <script src="dependencies/plugins/angular-sanitize/angular-sanitize.min.js" type="text/javascript"></script>
    <script src="dependencies/plugins/angular-oc-lazyload/ocLazyLoad.min.js" type="text/javascript"></script>
    <!-- END VENDOR JS -->
    <!-- BEGIN CORE TEMPLATE JS -->
    <script src="/publicApp/assets/js/app.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/config.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/config.lazyload.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/main.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/directives/pg-sidebar.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/directives/cs-select.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/directives/pg-dropdown.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/directives/pg-form-group.js" type="text/javascript"></script>
    <script src="/publicApp/pages/js/pages.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/directives/pg-navigate.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/directives/pg-portlet.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/directives/pg-tab.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/directives/pg-search.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/directives/skycons.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/controllers/search.js" type="text/javascript"></script>
    <script src="/publicApp/assets/js/services/RESTservice.js" type="text/javascript"></script>
    <!-- END CORE TEMPLATE JS -->
</body>

</html>